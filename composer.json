{
    "name":"easybib/form-decorator",
    "description": "EasyBib_Form_Decorator is a set of Twitter Bootstrap decorators for Zend_Form",
    "type":"library",
    "license":"Apache-2.0",
    "authors":[
        {
            "name": "Michael Scholl",
            "email": "michael@sch0ll.de"
        },
        {
            "name":"Till Klampaeckel",
            "email":"till@php.net"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "http://github.com/easybib/zf1"
        }
    ],
    "autoload": {
        "psr-0": {
            "EasyBib_Form": "library"
        }
    },
    "include-path": [
        "library/"
    ],
    "suggest": {
        "easybib-core/zf1": "Our Zend Framework 1 fork (sans require_once and a couple components we don't need/use)"
    },
    "require":{
        "php": ">=5.3.0"
    },
    "require-dev": {
        "easybib-core/zf1": "*",
        "phpunit/phpunit": "4.7.*"
    }
}
